{{#each contact}}
<div class="contacts" id="{{this.fname}}_{{this.lname}}">
	{{this.fname}} {{this.lname}}
	<div class="cont_info" id="{{this.fname}}_{{this.lname}}_info">

		<div class="cont_p_number">
		Phone Number<br>
		{{this.p_number}}
		</div>

		<div class="cont_location">
		{{this.location}}
		</div>

	</div>

</div>
{{/each}}
